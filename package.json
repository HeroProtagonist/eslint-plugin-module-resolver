{
  "name": "eslint-plugin-module-resolver",
  "version": "0.13.0",
  "description": "Warn when using relative paths to modules aliased",
  "repository": "HeroProtagonist/eslint-plugin-module-resolver",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "files": [
    "dist"
  ],
  "author": "Jordan",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "COLLECT_COVERAGE=true jest",
    "prebuild": "yarn clean",
    "build": "babel lib --out-dir dist",
    "build:watch": "yarn build --watch",
    "clean": "rimraf *.tgz dist",
    "bump": "yarn test && yarn version && yarn build && npm publish"
  },
  "dependencies": {
    "find-babel-config": "^1.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.4",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.2",
    "codecov": "^3.5.0",
    "eslint": "^6.2.1",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "regenerator-runtime": "^0.13.3",
    "rimraf": "^3.0.0"
  },
  "license": "MIT"
}
